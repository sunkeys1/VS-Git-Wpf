        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Binary_Multiply"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <UniformGrid x:Name="ufgA" Rows="1" Columns="8" Margin="39,62,454,319"/>
        <UniformGrid x:Name="ufgB" Rows="1" Columns="8" Margin="39,128,454,252"/>
        <TextBox x:Name="decA" HorizontalAlignment="Left" Height="38" Margin="387,62,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="175" TextChanged="DecA_TextChanged"/>
        <TextBox x:Name="decB" HorizontalAlignment="Left" Height="39" Margin="387,128,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="175"/>
